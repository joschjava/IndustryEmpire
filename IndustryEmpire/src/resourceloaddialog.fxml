<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ResourceLoadController">
   <children>
      <FlowPane fx:id="flowAllRes" prefHeight="200.0" prefWidth="200.0" />
      <ScrollPane prefViewportWidth="400.0">
         <content>
            <FlowPane fx:id="flowInputRes" hgap="5.0" vgap="5.0" />
         </content>
      </ScrollPane>
      <ProgressBar fx:id="vehLoad" progress="0.0" />
      <Button fx:id="btClose" mnemonicParsing="false" text="Close" />
   </children>
</VBox>
